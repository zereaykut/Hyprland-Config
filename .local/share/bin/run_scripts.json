{
    "SDDM-Theme-Manager": "python ~/.config/theme-manager/theme_manager_sddm.py",
    "Theme-Manager": "python ~/.config/theme-manager/theme_manager.py",
    "Power-Profile": "python ~/.local/share/bin/select_power_profile.py",
    "Web-Search": "python ~/.local/share/bin/web_search.py",
    "Hyprland-Window-Switcher": "python ~/.local/share/bin/hyprland_window_switcher.py",
    "Color-Scheme-Generator": "kitty -e python ~/.config/theme-manager/color_scheme_generator.py --service imagemagick",
    "Wallpaper-Cache-Generator": "kitty -e python ~/.config/theme-manager/wall_cache.py --service imagemagick",
    "Color-Scheme-Generator-Overwrite": "kitty -e python ~/.config/theme-manager/color_scheme_generator.py --service imagemagick --overwrite True",
    "Wallpaper-Cache-Generator-Overwrite": "kitty -e python ~/.config/theme-manager/wall_cache.py --service imagemagick --overwrite True",
    "Clipboard-Manager": "bash ~/.local/share/bin/clip_manager.sh",
    "Color-Picker": "bash ~/.local/share/bin/color_picker.sh"
}